{
  "name": "freezy-bee/editrouble",
  "type": "library",
  "description": "Content editable extension for Nette Framework.",
  "keywords": [
    "nette",
    "freezy-bee",
    "MediaEditor"
  ],
  "homepage": "http://freezybee.ifire.cz",
  "license": [
    "BSD-3-Clause",
    "GPL-2.0",
    "GPL-3.0"
  ],
  "authors": [
    {
      "name": "Jakub Janata",
      "homepage": "https://ifire.cz"
    }
  ],
  "require": {
    "php": ">=7.4",
    "nette/application": "~3.0",
    "nette/bootstrap": "~3.0",
    "nette/caching": "^3.0",
    "nette/security": "^3.0",
    "latte/latte": "~2.8"
  },
  "require-dev": {
    "dibi/dibi": "^4.0",
    "doctrine/orm": "^2.7",
    "phpstan/phpstan": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "FreezyBee\\Editrouble\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
